<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entidades" default-access="field">
	<class name="Pagamento" table="Pagamento">
		<id name="id" column="pagamento_id">
			<generator class="increment"/>
		</id>

		<property name="preco" />
		<property name="codigoPagamento" />
		<property name="estado" />
		
	<set lazy="false" name="pacote" table="pagamento_pacote">
			<key column="pagamento_id" />
			<many-to-many column="pacote_id" class="Pacote" />
		</set>
		
	</class>
</hibernate-mapping>
