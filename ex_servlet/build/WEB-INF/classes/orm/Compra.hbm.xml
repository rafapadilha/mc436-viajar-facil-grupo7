<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entidades" default-access="field">
	<class name="Compra" table="Compra">
		<id name="id" column="compra_id">
			<generator class="increment"/>
		</id>

		
		<set lazy="false" name="hotel" table="compra_hotel">
			<key column="compra_id" />
			<many-to-many column="hotel_id" class="ReservaHotel" />
		</set>
		

		<set lazy="false" name="pagamento" table="compra_pagamento">
			<key column="compra_id" />
			<many-to-many column="pagamento_id" class="Pagamento" />
		</set>
		

		<set lazy="false" name="guia" table="compra_guia">
			<key column="compra_id" />
			<many-to-many column="guia_id" class="GuiaTuristico" />
		</set>

		<set lazy="false" name="pacote" table="compra_pacote">
			<key column="compra_id" />
			<many-to-many column="pacote_id" class="Pacote" />
		</set>
		

		<set lazy="false" name="ListaPassagem" table="compra_passagem">
			<key column="compra_id" />
			<many-to-many column="passagem_id" class="Passagem" />
		</set>

	</class>
</hibernate-mapping>
